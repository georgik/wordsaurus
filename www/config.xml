<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sinusgear.saurus" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Wordsaurus</name>
    <description>
        Search in key/description dictionary stored in SQLite.
    </description>
    <author email="juraj.michalek@sinusgear.com" href="http://georgik.sinusgear.com">
        SinusGear
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <feature name="DatabaseManagerPlugin">
        <param name="android-package" value="com.sinusgear.saurus.DatabaseManagerPlugin" />
    </feature>

</widget>
